{
  "name": "api-top-active-user",
  "version": "0.0.0",
  "description": "RESTful api Domain Driven Design",
  "main": "index.js",
  "engines": {
    "node": ">=9",
    "npm": ">=5"
  },
  "scripts": {
    "start": "node cluster.js",
    "start:dev": "nodemon cluster.js",
    "start:cc": "codecrumbs -d src -e src/container.js",
    "test": "standard --verbose | snazzy && npm run db:refresh-test && npm run test:cov",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:cov": "cross-env NODE_PATH=. NODE_ENV=test istanbul cover node_modules/mocha/bin/_mocha -- --exit",
    "test:specs": "cross-env NODE_PATH=. NODE_ENV=test mocha --exit",
    "db:reset": "cross-env NODE_PATH=. && echo $NODE_PATH && npm run db:refresh && npm run seed",
    "db:refresh": "npm run migrate:sp:undo && npm run migrate:undo && npm run migrate && npm run migrate:sp",
    "db:refresh-test": "npm run migrate:sp:test:undo && npm run migrate:test:undo && npm run migrate:test && npm run migrate:sp:test",
    "db:refresh-sp": "npm run migrate:sp:undo && npm run migrate:sp",
    "db:refresh-sp-test": "npm run migrate:sp:test:undo && npm run migrate:sp:test",
    "migrate": "sequelize db:migrate",
    "migrate:test": "sequelize db:migrate --env test",
    "migrate:sp": "cross-env NODE_ENV=development sequelize db:migrate --migrations-path=./src/infra/sequelize/functions",
    "migrate:sp:test": "cross-env NODE_ENV=development sequelize db:migrate --env test --migrations-path=./src/infra/sequelize/functions",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "migrate:test:undo": "sequelize db:migrate:undo:all --env test",
    "migrate:sp:undo": "./cli/safe_undo_sp.js",
    "migrate:sp:test:undo": "./cli/safe_undo_sp.js --env=test",
    "seed": "cross-env NODE_ENV=development NODE_PATH=. sequelize db:seed:all",
    "seed:test": "cross-env NODE_ENV=development sequelize db:seed:all --env test",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "lint:staged": "lint-staged",
    "fresh-install": "rm -rf node_modules && npm cache clean --force && npm install",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release",
    "docker:script": "docker-compose run --rm web",
    "docker:db:reset": "npm run docker:script npm run db:reset",
    "docker:db:refresh": "npm run docker:script npm run db:refresh",
    "docker:db:refresh-sp": "npm run docker:script npm run db:refresh-sp",
    "docker:db:refresh-test": "npm run docker:script npm run db:refresh",
    "docker:db:refresh-sp-test": "npm run docker:script npm run db:refresh-sp-test",
    "docker:seed": "npm run docker:script npm run seed",
    "docker:test": "npm run docker:script npm run test",
    "docker:test:specs": "npm run docker:script npm run test:specs"
  },
  "lint-staged": {
    "*.js": "lint"
  },
  "pre-commit": "lint:staged",
  "author": "Joshua C Alpuerto <joshuacalpuerto@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "awilix": "4.2.0",
    "awilix-express": "2.1.1",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "5.2.0",
    "dotenv": "16.1.3",
    "express": "4.17.1",
    "express-status-monitor": "1.2.6",
    "faker": "4.1.0",
    "http-status": "1.3.1",
    "jsonwebtoken": "8.5.1",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "morgan": "1.9.1",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "pg": "7.11.0",
    "pm2": "3.5.1",
    "ramda": "0.26.1",
    "sequelize": "5.5.0",
    "swagger-jsdoc": "3.2.7",
    "tcomb": "3.2.29",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "chai": "4.2.0",
    "codecrumbs": "1.6.2",
    "coveralls": "3.0.5",
    "draxt": "0.6.0",
    "istanbul": "1.1.0-alpha.1",
    "lint-staged": "8.1.5",
    "nodemon": "1.19.1",
    "mocha": "6.1.4",
    "mocha-lcov-reporter": "1.3.0",
    "npm-check": "5.9.0",
    "pre-commit": "1.2.2",
    "semantic-release": "15.13.3",
    "sequelize-cli": "5.4.0",
    "sinon": "7.2.7",
    "sinon-chai": "3.3.0",
    "snazzy": "8.0.0",
    "standard": "^12.0.1",
    "supertest": "3.4.2",
    "travis-deploy-once": "5.0.11",
    "yargs": "13.2.2"
  },
  "standard": {
    "ignore": [
      "/public/docs"
    ],
    "globals": [
      "request",
      "app",
      "expect",
      "describe",
      "it",
      "config",
      "beforeEach"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuaalpuerto/api-top-active-user.git"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "logs/*",
      "redis-data/*",
      "postgres-data/*",
      "coverage/*"
    ],
    "delay": "1000"
  }
}
